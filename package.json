    },
    "dependencies": {
        "next": "^16.1.6",
        "zod": "^4.3.6"
    },
    "devDependencies": {
        "@eslint/js": "^10.0.1",
        "@types/next": "^8.0.7",
        "@types/node": "^25.1.0",
        "eslint": "^10.0.0",
        "globals": "^17.3.0",
        "husky": "^9.1.7",
        "jiti": "^2.6.1",
        "lint-staged": "^16.2.7",
        "ts-node": "^10.9.2",
        "typescript": "^5.9.3",
        "typescript-eslint": "^8.56.0"
    },
    "repository": {
        "url": "https://github.com/qualityapi/qualityapi",
        "type": "git"
    },
    "license": "Apache-2.0",
    "lint-staged": {
        "src/**/*.{ts,tsx}": "eslint"
    }
