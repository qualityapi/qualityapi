{
    "name": "qualityapi",
    "version": "1.0.0-alpha.6",
    "type": "module",
    "main": "dist/index.js",
    "types": "dist/types/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "require": "./dist/index.js",
            "types": "./dist/index.d.ts"
        },
        "./domain-router": {
            "import": "./dist/domain-router/index.js",
            "require": "./dist/domain-router/index.js",
            "types": "./dist/domain-router/index.d.ts"
        },
        "./utils": {
            "import": "./dist/utils/index.js",
            "require": "./dist/utils/index.js",
            "types": "./dist/utils/index.d.ts"
        },
        "./authentication": {
            "import": "./dist/authentication/index.js",
